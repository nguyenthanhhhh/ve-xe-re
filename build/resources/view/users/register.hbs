<form method="POST" action="/users/registerPOST">
    <h1>Đăng ký tài khoản</h1>
    <div class="form-group">
        <label for="name">Tài khoản</label>
        <input type="text" id="userName" class="form-control userNameForm" name="userName">

    </div>
    <div class="form-group">
        <label for="address">Mật khẩu</label>
        <input type="password" id="password" class="form-control passwordForm" name="password">
    </div>


    <div class="form-group">
        <label for="province">Nhập email</label>
        <input type="email" id="email" class="form-control emailForm" name="email">
    </div>

    <button type="submit" class="btn btn-primary">Đăng ký</button>
</form>